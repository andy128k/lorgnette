{
  "name": "lorgnette",
  "version": "1.0.0",
  "description": "Lenses. Helpers for immutable collections.",
  "keywords": [
    "lens",
    "immutable",
    "collection"
  ],
  "license": "MIT",
  "author": "Andrey Kutejko <andy128k@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/andy128k/lorgnette"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "babel --modules commonStrict --optional runtime src -d dist",
    "lint": "eslint src test && jscs src test",
    "pretest": "npm run lint && npm run build",
    "test": "istanbul cover _mocha --report lcov -- --ui bdd",
    "ci-test": "npm run test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "chai": "^3.2.0",
    "eslint": "^1.4.1",
    "istanbul": "^0.3.20",
    "jscs": "^2.5.0",
    "mocha": "^2.3.2"
  },
  "dependencies": {
    "babel-runtime": "^5.8.24"
  }
}
